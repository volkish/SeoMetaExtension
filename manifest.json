{
  "name": "SEO Meta",
  "version": "0.1.0.0",
  "manifest_version": 3,
  "description": "Show SEO information for the current page",
  "icons": {
    "16": "icons/icon-16x16.png",
    "32": "icons/icon-32x32.png",
    "128": "icons/icon-128x128.png"
  },
  "action": {
    "default_title": "Click to show SEO information"
  },
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  }
}